<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.0">

	<!-- HIV ART Register Module Properties -->
	<id>rwandareports</id>
	<name>Rwanda Reports</name>
	<version>1.0.1</version>
	<package>org.openmrs.module.@MODULE_ID@</package>
	<author>PIH</author>
	<description>Module to register reports for IMB Rwanda</description>
	<activator>@MODULE_PACKAGE@.RwandaReportsModuleActivator</activator>
	<updateURL>https://dev.openmrs.org/modules/download/@MODULE_ID@/update.rdf</updateURL>
	<require_version>1.4.0</require_version>
	

	<require_modules>
		<require_module>org.openmrs.module.rowperpatientreports</require_module>
	</require_modules>

	<extension>
		<point>org.openmrs.admin.list</point>
		<class>@MODULE_PACKAGE@.extension.html.ManageAdminListExt</class>
	</extension>

	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>

	<mappingFiles>
	</mappingFiles>
	
	<globalProperty>
		<property>reports.hivprogramname</property>
		<defaultValue>HIV PROGRAM</defaultValue>
		<description>
			The name of the HIV PROGRAM to be used
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.hivworkflowstatus</property>
		<defaultValue>TX STATUS</defaultValue>
		<description>
			The workflow status that is used to determine if patients are on ART
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.hivtreatmentstatus</property>
		<defaultValue>TREATMENT GROUP</defaultValue>
		<description>
			The workflow status that is used to determine the group of the HIV patients
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.hivonartstate</property>
		<defaultValue>ON ANTIRETROVIRALS</defaultValue>
		<description>
			The current state that signifies a patient is on ART's
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.weightConcept</property>
		<defaultValue>5089</defaultValue>
		<description>
			The concept id of the weight concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.cd4Concept</property>
		<defaultValue>5497</defaultValue>
		<description>
			The concept id of the cd4 count concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.patientDiedConcept</property>
		<defaultValue>1811</defaultValue>
		<description>
			The concept id of the patient died concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.cd4LabConcept</property>
		<defaultValue>657</defaultValue>
		<description>
			The concept id of the cd4 lab concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.labOrderType</property>
		<defaultValue>4</defaultValue>
		<description>
			The lab test order type
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pedihivprogramname</property>
		<defaultValue>PEDIATRIC HIV PROGRAM</defaultValue>
		<description>
			The program name for the pediatric hiv program
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctprogramname</property>
		<defaultValue>PMTCT PROGRAM</defaultValue>
		<description>
			The program name for the pmtct program
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.hivpreartstate</property>
		<defaultValue>FOLLOWING</defaultValue>
		<description>
			The current state that signifies a patient is pre ART's 
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.hivencountertypes</property>
		<defaultValue>ADULT INITIAL VISIT:ADULT RETURN VISIT:PEDIATRIC INITIAL VISIT:PEDIATRIC RETURN VISIT:ADULT HIV FLOWSHEET</defaultValue>
		<description>
			All of the encounter types that signify a HIV visit
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctGaveBirthStatus</property>
		<defaultValue>GAVE BIRTH</defaultValue>
		<description>
			The workflow status that indicates a pmtct patient has given birth
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.currentlocation</property>
		<defaultValue>Rwinkwavu Health Center</defaultValue>
		<description>
			The current location to be used by the reports
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctcombinedprogramname</property>
		<defaultValue>PMTCT Combined Clinic - Infant</defaultValue>
		<description>
			The Program name for the PMTCT Combined clinic
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.tbTestConcept</property>
		<defaultValue>2136</defaultValue>
		<description>
			The concept id of the TB test concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.allSecondLineArtDrugsConceptSet</property>
		<defaultValue>1085</defaultValue>
		<description>
			The concept id of the concept set that contains all Second Line ART drugs
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.allFirstLineArtDrugsConceptSet</property>
		<defaultValue>1085</defaultValue>
		<description>
			The concept id of the concept set that contains all First Line ART drugs
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pregnancyDeliveryDateConcept</property>
		<defaultValue>6188</defaultValue>
		<description>
			The concept id of pregnancy due date concept 
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.ctxTreatmentConcept</property>
		<defaultValue>916</defaultValue>
		<description>
			The concept id of the CTX Treatment drug concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.tbTreatmentConcept</property>
		<defaultValue>3014</defaultValue>
		<description>
			The concept id of all the TB Treatment drug concept set 
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.cd4PercentageConcept</property>
		<defaultValue>730</defaultValue>
		<description>
			The concept id of the cd4 percentage concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.stageConcept</property>
		<defaultValue>1480</defaultValue>
		<description>
			The concept id of the stage concept
		</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.allArtDrugsConceptSet</property>
		<defaultValue>1085</defaultValue>
		<description>
			The concept id of the concept set that contains all ART drugs
		</description>
	</globalProperty>
	
	<!-- Late Visit And CD$ Global Property -->
	<globalProperty>
		<property>ClinicalencounterTypeIds.labTestExcl</property>
		<defaultValue>1,2,3,4</defaultValue>
		<description>
			Clinical Encounter Type to find patients who have have not had a clinical encounter (lab test excluded)		</description>
	</globalProperty>
	
	<globalProperty>
		<property>hiv.programid.adult</property>
		<defaultValue>3</defaultValue>
		<description>Id for Adult HIV program</description>
	</globalProperty>
	
	<globalProperty>
		<property>hiv.programid.pediatric</property>
		<defaultValue>10</defaultValue>
		<description>Id for Pediatric HIV program</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.imbIdIdentifier</property>
		<defaultValue>Primary Care</defaultValue>
		<description>Identifier type that should be returned for the Primary Care</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.primaryCareIdIdentifier</property>
		<defaultValue>IMB ID</defaultValue>
		<description>Identifier type that should be returned for the IMB Id</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctMotherRelationship</property>
		<defaultValue>13</defaultValue>
		<description>The id of the Relationship that represents the mother in the PMTCT program</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctFeedingStatusWorkflowName</property>
		<defaultValue>Feeding status</defaultValue>
		<description>The name of the workflow for the feeding status</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctRelationshipStatusWorkflowName</property>
		<defaultValue>Relationship status</defaultValue>
		<description>The name of the workflow for the relationship status</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.accompagnatuerRelationship</property>
		<defaultValue>1</defaultValue>
		<description>The id of the Relationship that represents the accompagnatuer</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctNextVisitConcept</property>
		<defaultValue>5096</defaultValue>
		<description>The id of the concept for next pmtct visit</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.dbsConcept</property>
		<defaultValue>1553</defaultValue>
		<description>The id of the concept for the DBS test</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.viralLoadConcept</property>
		<defaultValue>856</defaultValue>
		<description>The id of the concept for the Viral Load test</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.pmtctDiscordantCoupleWorkflowState</property>
		<defaultValue>Discordant Couple</defaultValue>
		<description>The workflow state representing a discordant couple</description>
	</globalProperty>
	
	<globalProperty>
		<property>reports.labEncounterType</property>
		<defaultValue>9</defaultValue>
		<description>The id for the lab encounter type</description>
	</globalProperty>
	
	
	

	<!-- /Rwanda Report Module Properties -->
	
</module>
